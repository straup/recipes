<#>
         
  dc:title "Quick drop biscuits"; 

  e:ingredients (
     [e:amount "2"; e:measure I:cup; e:foodstuff "flour"; ]
     [e:amount "1"; e:measure I:tsp; e:foodstuff "salt"; ]
     [e:amount "2 1/2"; e:measure I:tsp; e:foodstuff "baking powder"; ]
     [e:amount "2-6"; e:measure I:Tbsp; e:foodstuff "butter"; e:detail "chilled";]
     [e:amount "1"; e:measure I:cup; e:foodstuff "milk"; ]
  );

  e:directions (
    [a:content """Preheat oven to 450 Farenheit.

Mix it up, good.

Plot large spoonfuls, to taste, on a baking sheet

Bake at 400 Farenheit for 20 minutes or until tops are crisp and golden.""";]
  ); 

  e:notes (
    [a:content """I took certain liberties with the original directions which were otherwise
long-winded and boring."""; 
    a:author "[[http://aaronstraupcope.com][asc]]"; 
    dc:date "2003-07-26T00:03:23 -0400"; ]
  );

  dc:source [
    dc:publication "The Joy of Cooking"; 
  ]; 

  a e:recipe ;
  dc:language "" ;
  dc:identifier "$Id: biscuits.n3,v 1.3 2007/12/26 02:20:41 asc Exp $" ;
  .
         