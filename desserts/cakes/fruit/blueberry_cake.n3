# -*-n3-*-

@prefix e:       <http://eatdrinkfeelgood.info/2.0#> .
@prefix I:       <http://eatdrinkfeelgood.info/2.0/us-imperial#> .
@prefix M:       <http://eatdrinkfeelgood.info/2.0/metric#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix a:       <http://purl.org/atom/1.0/> .

<#>
         
  dc:title "Blueberry Cake"; 

  e:ingredients (
     [e:amount "3"; e:foodstuff "eggs"; ]
     [e:amount "1/2"; e:measure I:cup; e:foodstuff "sugar"; ]
     [e:amount "3/4"; e:measure I:cup; e:foodstuff "oil"; ]
     [e:amount "3/4"; e:measure I:cup; e:foodstuff "orange juice"; ]
     [e:amount "3"; e:measure I:cup; e:foodstuff "flour"; e:detail "scant";]
     [e:amount "3"; e:measure I:tsp; e:foodstuff "baking powder"; ]
     [e:amount "1/2"; e:measure I:tsp; e:foodstuff "vanilla"; ]
     [e:amount "1"; e:measure I:pint; e:foodstuff "blueberries"; ]
  );

  e:directions (
    [a:content """Bake for 45 minutes at 350 degrees F."""]
  ); 

  e:notes (
    [a:content """There are no other directions, so I guess the only thing to do is mix everything in a
bowl and pour it into a cake pan."""; 
    a:author "[[http://www.aaronstraupcope.com/whois][asc]]"; 
    dc:date "20030615T12:52:00-0500"; ]
  );

  a e:recipe ;
  dc:language "" ;
