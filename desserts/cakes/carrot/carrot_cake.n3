# -*-n3-*-

@prefix e:       <http://eatdrinkfeelgood.info/2.0#> .
@prefix I:       <http://eatdrinkfeelgood.info/2.0/us-imperial#> .
@prefix M:       <http://eatdrinkfeelgood.info/2.0/metric#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix a:       <http://purl.org/atom/1.0/> .

<#>
         
  dc:title "Carrot Cake"; 

  e:ingredients (
     [e:amount "3"; e:measure I:cup; e:foodstuff "carrots"; e:detail "grated";]
     [e:amount "2"; e:measure I:cup; e:foodstuff "flour"; ]
     [e:amount "2"; e:measure I:tsp; e:foodstuff "baking soda"; ]
     [e:amount "1"; e:measure I:tsp; e:foodstuff "salt"; ]
     [e:amount "2"; e:measure I:cup; e:foodstuff "sugar"; ]
     [e:amount "2"; e:measure I:tsp; e:foodstuff "cinnamon"; ]
     [e:amount "1 1/2"; e:measure I:cup; e:foodstuff "oil"; ]
     [e:amount "4"; e:foodstuff "eggs"; ]
     [e:amount "1/2"; e:measure I:cup; e:foodstuff "walnuts"; ]
     [e:amount "8"; e:measure I:oz; e:foodstuff "cream cheese"; ]
     [e:amount "1"; e:measure "stick"; e:foodstuff "butter"; e:detail "room temperature";]
     [e:amount "1"; e:measure "box"; e:foodstuff "Confectioners sugar"; ]
     [e:amount "2"; e:measure I:tsp; e:foodstuff "sugar"; ]
     [e:amount "1"; e:measure I:tsp; e:foodstuff "vanilla extract"; ]
  );

  e:directions (
    [a:content """Sift together the dry ingredients. Add the oil and stir. Add the eggs and stir. Add the
grated carrots and stir. If planning to serve the cake without icing, add the nuts to
the cake batter.

Bake in three 9-inch round pans or a large rectangular pan at 350 F for 35 minutes.

To make the frosting, combine the cream cheese, butter, Confectioners sugar, two
Tbsp sugar and vanilla

When cool, frost the cake and sprinkle with the walnuts.""";]
  ); 

  e:history (
    [a:content """Weekends when Moby Hill was filled with friends there was always a carrot cake in the
wings, but not for long. We've known certain guests to get up early to have a piece before breakfast!"""; 
    a:author "[[#][PatÂ Novak]]"; ]
  );

  dc:source [
    dc:publication "Dottie's Cookbook"; 
  ]; 

  a e:recipe ;
  dc:language "" ;
  dc:identifier "$Id: carrot_cake.n3,v 1.3 2007/12/26 02:20:41 asc Exp $" ;
  .
         