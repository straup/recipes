@prefix e:       <http://eatdrinkfeelgood.info/2.0#> .
@prefix I:       <http://eatdrinkfeelgood.info/2.0/us-imperial#> .
@prefix M:       <http://eatdrinkfeelgood.info/2.0/metric#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix a:       <http://purl.org/atom/1.0/> .

<#>
         
  dc:title "Prosciutto, Fig and Parmesan Rolls"; 

  e:ingredients (
     [e:amount "10"; e:measure "slices"; e:foodstuff "prosciutto"; e:detail "very thin";]
     [e:amount "1/4"; e:measure I:pound; e:foodstuff "Prmesan"; e:detail "sliced wafer thin";]
     [e:amount "8-10"; e:foodstuff "dried figs"; e:detail "stemmed and cut into about 3 strips each";]
     [e:foodstuff "Truffle or olive oil"; ]
  );

  e:directions (
    [a:content """Cut each piece of prosciutto in half the long way; on each piece, put a slice of
Parmesan and strip of fig. Roll up the long way, pressing so meat sticks to itself. Cover with
plastic wrap if not serving right away.

Serve within 2 hours, drizzling truffle or olive oil on top just before serving.""";]
  ); 

  e:yield [
    e:makes " "; 
  ];

  dc:source [
    dc:publication "New York Times"; 
  ]; 

  a e:recipe ;
  dc:language "" ;
  dc:identifier "$Id: prosciutto_fig_parm_rolls.n3,v 1.2 2007/12/26 02:20:42 asc Exp $" ;
  .
         