<#>
         
  dc:title "Marine Stew"; 

  e:ingredients (
     [e:amount "1"; e:measure I:pound; e:foodstuff "fresh cod"; e:detail "fillet";]
     [e:amount "1/2"; e:measure I:pint; e:foodstuff "oysters"; ]
     [e:measure "pat"; e:foodstuff "butter"; ]
     [e:amount "1"; e:measure I:qt; e:foodstuff "milk"; ]
     [e:amount "4"; e:measure I:Tbsp; e:foodstuff "dulse"; e:detail "finely chopped";]
     [e:amount "1"; e:measure I:cup; e:foodstuff "spinach"; e:detail "coarsely chopped";]
     [e:foodstuff "basil"; e:detail "to taste";]
     [e:foodstuff "paprika"; e:detail "to taste";]
  );

  e:directions (
    [a:content """Poach the fish briefly in small amount of water. Add oysters and butter. Simmer
gently 'til oysters are firm (about 3-4 minutes). May add spoonful of dry sherry.

Pour in the milk slowly. Stir carefully with wooden spoon. Try to leave the fish in
generous pieces or large flakes. Add the dulce and spinach, then pinch of basil, salt and pepper.

Bring up to scalding but do not boil. Sprinke with paprika; cover and let sit up to 1/2
hour to develop flavour. Reheat if necessary and serve with noodles au gratin and/or
three-bean salad."""]
  ); 

  e:yield [
    e:makes " "; 
  ];

  e:notes (
    [a:content """The recipe indicates that "Dulce is dry red alga available in most supermarkets or
health food stores. Imparts flavour; good source of iodine."""; 
    a:author "[[#][AaronÂ Cope]]"; 
    dc:date "2001-11-23T00:00:00 -0500"; ]
  );

  dc:source [
    dc:publication "Moby Hill (Lucille Naples Weston)"; 
  ]; 

