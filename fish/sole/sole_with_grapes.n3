  dc:title "Filet of Sole with Grapes"; 

  e:ingredients (
     [e:amount "1/2"; e:measure I:pound; e:foodstuff "sole"; e:detail "filets";]
     [e:amount "1"; e:measure I:Tbsp; e:foodstuff "butter"; ]
     [e:amount "3"; e:measure "large"; e:foodstuff "mushrooms"; e:detail "sliced";]
     [e:amount "24"; e:foodstuff "grapes"; e:detail "seedless";]
     [e:amount "1/4"; e:measure I:cup; e:foodstuff "sour cream"; ]
     [e:amount "1/4"; e:measure I:cup; e:foodstuff "mayonnaise"; ]
     [e:amount "1/4"; e:foodstuff "lemon"; e:detail "juiced";]
  );

  e:directions (
    [a:content """Preheat ove to 350 F.

Generously butter au gratin or shallow dish. Arrange fish in dish.

Saute mushrooms in butter until softened. Add grapes, stirring gently.

Add remaining ingredients and blend well. Spoon over fish.

Tent with foil and bake about 20 minutes.""";]
  ); 

