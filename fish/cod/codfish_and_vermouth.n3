@prefix M:       <http://eatdrinkfeelgood.info/2.0/metric#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix a:       <http://purl.org/atom/1.0/> .

<#>
         
  dc:title "Cod Fish and Vermouth"; 

  e:ingredients (
     [e:amount "6"; e:measure "fillets"; e:foodstuff "cod"; ]
     [e:amount "2"; e:measure I:Tbsp; e:foodstuff "olive oil"; ]
     [e:measure "few sprigs"; e:foodstuff "oregano"; ]
     [e:amount "2"; e:measure I:Tbsp; e:foodstuff "Italian parsley"; e:detail "chopped";]
     [e:amount "2"; e:measure I:Tbsp; e:foodstuff "almonds"; e:detail "blanched and slivered";]
     [e:amount "1/2"; e:measure I:cup; e:foodstuff "dry vermouth"; ]
  );

  e:directions (
    [a:content """Brush the cod fillets with oil. Lay them in a baking dish. Sprinkle with salt, pepper,
oregano and parsley. Scatter nuts on top.

Pour vermouth around the fish. Bake at 350 F for 10 minutes or until fish just begins to flake.

Serve the fish from the dish and spoon the juices over it. Serve with lemon wedges.""";]
  ); 

  e:yield [
    e:makes " "; 
  ];

  e:notes (
    [a:content """The recipe indicates that you can also use halibut, whiting or monkfish with this recipe."""; 
    a:author "[[#][AaronÂ Cope]]"; 
    dc:date "2001-11-24T00:00:00 -0500"; ]
  );

  dc:source [
    dc:publication "Moby Hill"; 
  ]; 

  a e:recipe ;
